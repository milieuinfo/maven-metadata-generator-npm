omg_ont:dcat-ap-constraints
    a               owl:Ontology ;
    rdfs:comment
        "Specifieert hoe de data in de DCAT metadata te encoderen in RDF (i.e. RDF termen, kardinaliteiten,...)."@nl ;
    rdfs:label      "DCAT AP Constraints"@nl ;
    dct:abstract    "Het dcat applicatieprofiel specifieert de RDF termen (klassen en properties) en bijhorende constraints die gebruikt worden voor het beschrijven van matrices. Het maakt hierbij zoveel mogelijk gebruik van bestaande termen (klassen en properties) uit bestaande , skos, dbo, dbp en een minimale set van nieuwe termen gedefinieerd in het DCAT Vocabularium."@nl ;
    dct:alternative "DCAT AP Shapes"@nl ;
    dct:creator     omg_ag_cl:creator ;
    dct:description "Dit is de SHACL encodering van het applicatieprofiel van een dcat."@nl ;
    dct:issued      "2022-10-03"^^xsd:date ;
    dct:title       "DCAT AP Constraints"@nl ;
    xkos:belongsTo  omg_dataset:codelijst-dcat ;
    xhv:last        omg_voc:dcat-ap-constraints ;
    owl:sameAs      omg_voc:dcat-ap-constraints ;
    owl:versionInfo "Versie 1.0.0  oktober 2023"@nl .


omg_voc:dcat-ap-constraints
    a          owl:Ontology ;
    rdfs:label "DCAT AP Constraints"@nl ;
    owl:sameAs omg_ont:dcat-ap-constraints .

omg_ag_cl:creator
    rdfs:label "Ontwikkelaar codelijst"@nl ;
    schema:affiliation vl_org:OVO003751 ;
    a foaf:Person;
    foaf:firstName "Geert";
    foaf:lastName "Van Haute";
    foaf:mbox <mailto:geert.vanhaute@vlaanderen.be>.



# Node Shapes
ons:catalog a                sh:NodeShape ;
    rdfs:label       "vormvereisten voor de catalogus van codelijsten"@nl ;
    sh:name          "vormvereisten voor de catalogus van codelijsten"@nl ;
    sh:description   "De shacl/ontologie die de vormvereisten voor een catalogus beschrijft"@nl ;
    rdfs:isDefinedBy omg_ont:dcat-ap-constraints, omg_voc:dcat-ap-constraints ;
    sh:property      omg_ps:dcterms_hasPart, omg_ps:dcat_dataset, omg_ps:rdf_type, omg_ps:rdfs_label, omg_ps:dcterms_title,
                         omg_ps:dcat_service, omg_ps:dcterms_description, omg_ps:dcat_resource ;
    sh:closed true ;
    sh:targetClass   dcat:Catalog .

ons:dataset a                sh:NodeShape ;
    rdfs:label       "dataset"@nl ;
    sh:name          "dataset"@nl ;
    sh:description   "A collection of data, published or curated by a single source, and available for access or download in one or more representations."@nl ;
    rdfs:isDefinedBy omg_ont:dcat-ap-constraints, omg_voc:dcat-ap-constraints ;
    sh:property     omg_ps:dcat_accessURL, omg_ps:dcterms_isPartOf, omg_ps:dcterms_subject,
                        omg_ps:rdf_type,omg_ps:dcat_contactPoint,omg_ps:dcat_landingPage,omg_ps:dcat_theme,
                        omg_ps:dc_identifier,omg_ps:dcterms_accessRights,omg_ps:dcterms_accrualPeriodicity,
                        omg_ps:dcterms_alternative,omg_ps:dcterms_description,omg_ps:dcterms_hasVersion,omg_ps:dcterms_identifier,
                        omg_ps:dcterms_language,omg_ps:dcterms_license,omg_ps:dcterms_publisher,omg_ps:dcterms_rightsHolder,
                        omg_ps:dcterms_spatial,omg_ps:dcterms_temporal,omg_ps:dcterms_title,omg_ps:foaf_page, omg_ps:dcterms_provenance,
                        omg_ps:rdfs_label,omg_ps:void_rootResource, omg_ps:ssd_defaultGraph ;
    sh:closed true ;
    sh:targetSubjectsOf dcterms:hasVersion .


ons:dataset_version a                sh:NodeShape ;
    rdfs:label       "dataset versie"@nl ;
